<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="750px"
                                             android:layout_height="1134px"
    android:background="@color/colorAccent"
                                             tools:context="com.lvshou.sketchlayout.MainActivity">


    <TextView
        android:id="@+id/rectanglecopy2"
        android:layout_width="690px"
        android:layout_height="100px"
        android:text="rectanglecopy2"
        android:textColor="#ff000000"
        android:background="#ff261e44"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="346px"
        />
    <TextView
        android:id="@+id/rectangle"
        android:layout_width="690px"
        android:layout_height="100px"
        android:text="rectangle"
        android:textColor="#ff000000"
        android:background="#ffb492c1"
