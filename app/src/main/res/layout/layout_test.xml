        />
    <TextView
        android:id="@+id/shape"
        android:layout_width="128px"
        android:layout_height="84px"
        android:text="shape"
        android:textColor="#ff000000"
        android:background="#ffb86974"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="5px"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8px"

        />
    <TextView
        android:id="@+id/denglu"
        android:layout_width="112px"
        android:layout_height="78px"
        android:text="登录"
        android:textColor="#FF333333"
        android:background="#ff573977"
        app:layout_constraintLeft_toLeftOf="@+id/line1321162419"
        android:layout_marginLeft="0px"
        app:layout_constraintBottom_toTopOf="@+id/rectangle"
        android:layout_marginBottom="79px"

        />
    <TextView
        android:id="@+id/qingshurushoujihao"
        android:layout_width="186px"
        android:layout_height="42px"
        android:text="请输入手机号"
        android:textColor="#FF818DCF"
        android:background="#ff09220c"
        app:layout_constraintLeft_toLeftOf="@+id/line1321162419"
        android:layout_marginLeft="0px"
        app:layout_constraintTop_toTopOf="@+id/rectangle"
        app:layout_constraintBottom_toBottomOf="@+id/line1321162419"
        />
    <TextView
        android:id="@+id/shape306303911"
        android:layout_width="63px"
        android:layout_height="84px"
        android:text="shape306303911"
        android:textColor="#ff000000"
        android:background="#ff895ec3"
        app:layout_constraintRight_toRightOf="@+id/rectangle4"
        android:layout_marginRight="42px"
        app:layout_constraintBottom_toTopOf="@+id/shape"
        android:layout_marginBottom="128px"

        />

</android.support.constraint.ConstraintLayout>
