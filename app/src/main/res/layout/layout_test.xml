<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="750px"
                                             android:layout_height="1134px"
    android:background="@color/colorAccent"
                                             tools:context="com.lvshou.sketchlayout.MainActivity">

    <TextView
        android:id="@+id/rectangle4"
        android:layout_width="600px"
        android:layout_height="90px"
        android:text="rectangle4"
        android:background="#ff25bbae"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="502px"

        />
    <TextView
        android:id="@+id/huaiyingqinglaidaohaoxiangshou"
        android:layout_width="464px"
        android:layout_height="81px"
        android:text="欢迎你来到好享瘦"
        android:background="#ffd050d6"
        app:layout_constraintLeft_toLeftOf="@+id/rectangle4"
        app:layout_constraintRight_toLeftOf="@+id/rectangle4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle4"
        />
    <TextView
        android:id="@+id/suibiankankan"
        android:layout_width="136px"
        android:layout_height="48px"
        android:text="随便看看"
        android:background="#ffbbf0b2"
        app:layout_constraintLeft_toLeftOf="@+id/huaiyingqinglaidaohaoxiangshou"
        app:layout_constraintRight_toLeftOf="@+id/huaiyingqinglaidaohaoxiangshou"
        app:layout_constraintTop_toTopOf="@+id/huaiyingqinglaidaohaoxiangshou"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle4"
        />
    <TextView
        android:id="@+id/denglu"
        android:layout_width="82px"
        android:layout_height="48px"
        android:text="登录"
        android:background="#ff97a4a6"
        app:layout_constraintLeft_toLeftOf="@+id/suibiankankan"
        app:layout_constraintRight_toLeftOf="@+id/huaiyingqinglaidaohaoxiangshou"
        app:layout_constraintTop_toTopOf="@+id/rectangle4"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle4"
        />
    <TextView
        android:id="@+id/zhuce"
        android:layout_width="82px"
        android:layout_height="48px"
        android:text="注册"
        android:background="#ffee59aa"
        app:layout_constraintLeft_toLeftOf="@+id/denglu"
        app:layout_constraintRight_toLeftOf="@+id/denglu"
        app:layout_constraintTop_toTopOf="@+id/denglu"
        app:layout_constraintBottom_toBottomOf="parent"
        />
    <TextView
        android:id="@+id/weixindenglu"
        android:layout_width="101px"
        android:layout_height="37px"
        android:text="微信登录"
        android:background="#fff3bfe5"
        app:layout_constraintLeft_toLeftOf="@+id/zhuce"
        app:layout_constraintRight_toLeftOf="@+id/rectangle4"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="57px"

        />
    <TextView
        android:id="@+id/fill1"
        android:layout_width="38px"
        android:layout_height="31px"
        android:text="fill1"
        android:background="#ff793504"
        app:layout_constraintLeft_toLeftOf="@+id/zhuce"
        app:layout_constraintRight_toLeftOf="@+id/huaiyingqinglaidaohaoxiangshou"
        app:layout_constraintTop_toTopOf="@+id/weixindenglu"
        app:layout_constraintBottom_toBottomOf="@+id/weixindenglu"
        />
    <TextView
        android:id="@+id/line"
        android:layout_width="3px"
        android:layout_height="31px"
        android:text="line"
        android:background="#ff1be1fc"
        app:layout_constraintLeft_toLeftOf="@+id/zhuce"
        app:layout_constraintRight_toLeftOf="@+id/zhuce"
        app:layout_constraintTop_toTopOf="@+id/fill1"
        app:layout_constraintBottom_toBottomOf="@+id/weixindenglu"
        />
    <TextView
        android:id="@+id/qqdenglu"
        android:layout_width="89px"
        android:layout_height="37px"
        android:text="QQ登录"
        android:background="#ff9b4d77"
        app:layout_constraintRight_toLeftOf="@+id/zhuce"
        android:layout_marginRight="44px"
        app:layout_constraintTop_toTopOf="@+id/weixindenglu"
        app:layout_constraintBottom_toBottomOf="@+id/weixindenglu"
        />


</android.support.constraint.ConstraintLayout>
