        />
    <TextView
        android:id="@+id/line"
        android:layout_width="690px"
        android:layout_height="2px"
        android:text="line"
        android:textColor="#ff000000"
        android:background="#33818DCF"
        app:layout_constraintLeft_toLeftOf="@+id/rectangle"
        app:layout_constraintRight_toRightOf="@+id/rectangle"
        app:layout_constraintTop_toBottomOf="@+id/rectanglecopy2"
        android:layout_marginTop="0px"
